{
  "name": "swaeduae",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:init": "node scripts/init-postgres.js",
    "db:test": "node scripts/test-db-connection.js",
    "db:backup": "node scripts/backup-database.js",
    "db:backup:compressed": "node scripts/backup-database.js compressed",
    "db:backup:full": "node scripts/backup-database.js full",
    "db:backup:list": "node scripts/backup-database.js list",
    "db:backup:clean": "node scripts/backup-database.js clean",
    "health": "node scripts/health-check.js",
    "health:json": "node scripts/health-check.js --json",
    "docker:build": "docker build -f Dockerfile.production -t swaeduae:latest .",
    "docker:run": "docker-compose -f docker-compose.production.yml up -d",
    "docker:stop": "docker-compose -f docker-compose.production.yml down",
    "docker:logs": "docker-compose -f docker-compose.production.yml logs -f",
    "production:setup": "npm run db:init && npm run health",
    "production:deploy": "npm run build && npm run docker:build && npm run docker:run",
    "production:backup": "npm run db:backup:full",
    "production:health": "npm run health:json"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^3.8.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@sentry/nextjs": "^10.22.0",
    "@sentry/node": "^10.22.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/qrcode": "^1.5.6",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "import-in-the-middle": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.468.0",
    "next": "^15.5.6",
    "nodemailer": "^7.0.10",
    "pg": "^8.13.1",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^19.2.0",
    "recharts": "^3.3.0",
    "require-in-the-middle": "^8.0.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/jspdf": "^2.0.0",
    "@types/node": "^20.17.6",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.3",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
